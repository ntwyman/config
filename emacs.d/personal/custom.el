(defconst my-theme 'monokai)
(disable-theme 'zenburn)
(setq prelude-theme my-theme)
(if (daemonp)
    (add-hook 'after-make-frame-functions
              (lambda (frame)
                (select-frame frame)
                (load-theme my-theme t)))
  (load-theme my-theme t))

